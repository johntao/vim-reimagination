      "request": "launch",
      "projectPath": "${workspaceFolder}/app/vim-reimagination.csproj"
    },
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "BuildVimReimagination",
